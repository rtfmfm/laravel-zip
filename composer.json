{
  "name": "rtfmfm/laravel-zip",
  "description": "laravel-zip is the world's leading zip utility for file compression and backup.",
  "license": "MIT",
  "authors": [
    {
      "name": "rtfmfm",
    }
  ],
  "keywords": [
    "laravel-zip",
    "laravel",
    "laravel5",
    "zip",
    "unzip",
    "extract",
    "backup",
    "ZipArchive",
    "multiple",
    "merge"
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Rtfmfm\\Zip\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rtfmfm\\Zip\\ZipServiceProvider"
      ],
      "aliases": {
        "Zip": "Rtfmfm\\Zip\\ZipFacade"
      }
    }
  }
}
